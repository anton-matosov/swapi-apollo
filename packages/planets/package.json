  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/debug": "0.0.29",
    "@types/graphql": "^0.9.1",
    "@types/mocha": "^2.2.41",
    "@types/request": "0.0.45",
    "chai": "^4.0.2",
    "debug": "^2.6.8",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "replayer": "^2.1.3",
    "rimraf": "^2.6.1",
    "tslint": "^5.3.2",
    "typescript": "^2.3.3"
  },
